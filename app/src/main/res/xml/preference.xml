<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@drawable/activity_bckgrnd">

    <ListPreference
        android:title="Change University"
        android:key="edu.osu.uvento.user_univ_id"
        android:defaultValue="1"
       />

    <SwitchPreference
        android:title="Notifications"
        android:key="notifyOnOff"></SwitchPreference>

    <ListPreference
        android:title="Notify On"
        android:key="notify"
        android:defaultValue="1"
        android:entries="@array/notificationArray"
        android:entryValues="@array/notificationValues"
        />

    <edu.osu.uvento.controller.others.TimePickerPreference
        android:title="Notification Time"
        android:key="notifyTime"
        />


    <ListPreference
        android:title="My Event Expiration"
        android:key="eventExpiration"
        android:defaultValue="1"
        android:entries="@array/eventExpirationArray"
        android:entryValues="@array/eventExpirationValues"
        />

</PreferenceScreen>
